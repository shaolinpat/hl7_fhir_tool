           rdfs:subPropertyOf fhir:gender ;
           rdfs:domain hft:Patient ;
           rdfs:range hft:AdministrativeGenderCode ;
           rdfs:comment "Administrative gender of the patient, representing the gender recorded for administrative or clinical purposes (male, female, other, unknown). This may differ from gender identity and is used in clinical, epidemiolical, and operational contexts." ;
           rdfs:label "administrative gender" .


###  http://example.org/hl7-fhir-tool#hasCode
hft:hasCode rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( hft:Condition
                                        hft:Observation
                                      )
                        ] ;
            rdfs:range rdfs:Resource ;
            rdfs:comment "Links an Observation or a Condition to its concept code IRI (e.g., LOINC or ICD-10)." ;
            rdfs:label "has code" .


###  http://example.org/hl7-fhir-tool#hasCondition
hft:hasCondition rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf fhir:Encounter.reasonReference ;
                 rdfs:domain hft:Encounter ;
                 rdfs:range hft:Condition ;
                 rdfs:comment "Convenience link: Encounter -> Condition (aliasing the idea of diagnosis/reason). In FHIR RDF this is normally nested under Encounter.diagnosis.* or via reasonReference." ;
                 rdfs:label "has condition" .


###  http://example.org/hl7-fhir-tool#hasPart
hft:hasPart rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf fhir:DiagnosticReport.result ;
            owl:inverseOf hft:partOf ;
            rdfs:domain hft:DiagnosticReport ;
            rdfs:range hft:Observation ;
            rdfs:comment "DiagnosticReport includes one or more Observations; aligns to fhir:DiagnosticReport.result." ;
            rdfs:label "has part" .


###  http://example.org/hl7-fhir-tool#hasSubject
hft:hasSubject rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf fhir:subject ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
