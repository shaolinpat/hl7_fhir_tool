                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain hft:Condition ;
                     rdfs:range hft:Patient ;
                     rdfs:comment "Links a Condition to the Patient it concerns; functional subPropertyOf hft:hasSubject." ;
                     rdfs:label "condition subject" .


